{
  "name": "acme-writers-group",
  "version": "1.0.0",
  "description": "Before using running this server:    - [Setup postgres locally.](https://www.postgresql.org/download/)   - [Ensure NodeJS is installed.](https://docs.npmjs.com/downloading-and-installing-node-js-and-npm)   - Install dependencies using `npm i`.   - Seed database using `npm seed`.",
  "main": "server.js",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pgtools": "^0.3.2",
    "sequelize": "^6.21.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-react": "^7.17.12",
    "@faker-js/faker": "^7.3.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "test": "mocha spec.js",
    "test:dev": "DATABASE_URL=postgres://localhost/acme-writers-group_test npm run test -- --watch",
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "start": "node server.js",
    "start:dev": "npm run start-server & npm run build:dev",
    "start-server": "nodemon server --ignore dist --ignore src",
    "seed": "node seeder.js"
  },
